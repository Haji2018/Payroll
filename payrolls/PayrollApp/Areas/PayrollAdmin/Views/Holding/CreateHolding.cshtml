
@using PayrollApp.Models.Head
@model Holding



<div class="Holding col-md-12">
    <div class="head-holding">
        <div class="row">

            <p> Holding Yarat</p>




        </div>

    </div>

    <form asp-action="CreateHolding" method="post" asp-controller="Holding" class="col-md-11">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group col-md-12">
            <label asp-for="HoldingName" class="control-label"></label>
            <input asp-for="HoldingName" class="form-control holdingControl" />
            <span asp-validation-for="HoldingName" class="text-danger"></span>
        </div>
        <div class="form-group col-md-12" >
            <label asp-for="Address" class="control-label"></label>
            <input asp-for="Address" class="form-control holdingControl" />
            <span asp-validation-for="Address" class="text-danger"></span>
        </div>
        <div class="form-group col-md-12">
            <label asp-for="Phone" class="control-label"></label>
            <input asp-for="Phone" class="form-control holdingControl" />
            <span asp-validation-for="Phone" class="text-danger"></span>
        </div>
        <div class="form-group col-md-12">
            <label asp-for="About" class="control-label"></label>
            <input asp-for="About" class="form-control holdingControl" />
            <span asp-validation-for="About" class="text-danger"></span>
        </div>
        <div class="form-group">
            <input type="submit" value="Əlavə Et" class="btn btn-primary " /> |
            <a class=" btn listbtn" asp-action="ListHolding">Geri Qayıt</a>
        </div>
    </form>
</div>






